{
  "name": "#{name}",
  "description": "#{description}",
  "license": "#{license.name}",
  "licenseStart": "#{license.year}",
  "scripts": {
    "start": "tunnckocore-scripts",
    "extends": "@tunnckocore/config/scripts"
  },
  "engines": {
    "node": "^8.11.0 || >=10.13.0"
  },
  "dependencies": #{deps},
  "devDependencies": #{devDeps},
  "files": [
    "src",
    "index.js"
  ],
  "main": "index.js",
  "module": "src/index.js",
  "typings": "src/index.d.ts",
  "version": "#{version}",
  "repository": "#{repository}",
  "homepage": "https://github.com/#{repository}",
  "author": "#{author.name} (#{author.url})",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "renovate": {
    "extends": "tunnckocore"
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 4
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "asia",
        "charlike",
        "docks",
        "execa"
      ]
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "new-release",
      "semantic-release"
    ]
  }
}
