{
  "name": "<%= project.name %>",
  "description": "<%= project.description %>",
  "license": "<%= license.name %>",
  "licenseStart": "<%= license.year %>",
  "scripts": {
    "docs": "scripts docks --outfile .verb.md && scripts verb",
    "lint": "scripts eslint '**/*.js' --cache --fix --quiet --format codeframe",
    "test-only": "node -r esm test",
    "test": "nyc node test",
    "precommit": "scripts lint && scripts test-only",
    "commit": "scripts dry",
    "dry": "git add -A && git status --porcelain && scripts gitcommit"
  },
  "engines": {
    "node": "^8.10.0 || >=10.13.0"
  },
  "dependencies": <%= deps %>,
  "devDependencies": <%= devDeps %>,
  "files": [
    "src",
    "index.js"
  ],
  "main": "index.js",
  "module": "src/index.js",
  "typings": "src/index.d.ts",
  "version": "<%= version %>",
  "repository": "<%= repository %>",
  "homepage": "<%= project.homepage %>",
  "author": "<%= author.name %> (<%= author.url %>)",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "renovate": {
    "extends": "tunnckocore"
  },
  "eslintConfig": {
    "extends": "tunnckocore"
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 4
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "asia",
        "charlike",
        "docks",
        "gitcommit",
        "git-commits-since",
        "recommended-bump",
        "@tunnckocore/create-project"
      ]
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "new-release",
      "semantic-release"
    ]
  }
}
