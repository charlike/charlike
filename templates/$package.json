{
  "name": "#{name}",
  "description": "#{description}",
  "license": "Apache-2.0",
  "licenseStart": "#{date('yyyy')}",
  "scripts": {
    "lint": "eslint src test --fix --quiet",
    "test": "nyc --require esmc/register asia",
    "commit-dry": "git status --porcelain && git add -A && gitcommit -sS",
    "precommit": "yarn lint && yarn test",
    "commit": "yarn commit-dry",
    "build": "esmc",
    "prepublishOnly": "yarn build"
  },
  "engines": {
    "node": "^10.13.0"
  },
  "dependencies": {
    "esm": "^3.0.84"
  },
  "devDependencies": {
    "all-contributors-cli": "^5.4.0",
    "asia": "^0.19.7",
    "eslint-config-esmc": "^1.0.11",
    "gitcommit": "^1.0.9",
    "new-release": "^4.0.3",
    "nyc": "^12.0.2"
  },
  "files": [
    "src",
    "index.js"
  ],
  "main": "index.js",
  "module": "src/index.js",
  "version": "0.0.0-semantically-released",
  "repository": "#{owner}/#{name}",
  "homepage": "https://github.com/#{owner}/#{name}",
  "author": "Charlike Mike Reagent (https://tunnckocore.com)",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "eslintConfig": {
    "extends": "esmc"
  },
  "renovate": {
    "extends": "tunnckocore"
  },
  "nyc": {
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "lines": 0,
    "cache": true,
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "test"
    ]
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 4
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "asia",
        "gitcommit",
        "new-release",
        "docks",
        "charlike"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
