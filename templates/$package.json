{
  "name": "#{name}",
  "description": "#{description}",
  "nspId": "todo",
  "license": "Apache-2.0",
  "licenseStart": 2018,
  "scripts": {
    "lint": "hela lint",
    "test": "hela test",
    "commit": "hela commit --no-docs",
    "prepublishOnly": "hela protect"
  },
  "engines": {
    "node": ">=8.6",
    "npm": ">=5.2",
    "yarn": ">=1.3"
  },
  "dependencies": {},
  "devDependencies": {
    "hela": "^2.0.9",
    "hela-config-tunnckocore": "^3.0.16",
    "mukla": "^0.4.9"
  },
  "files": [
    "dist"
  ],
  "eslintConfig": {
    "extends": "tunnckocore"
  },
  "renovate": {
    "extends": "tunnckocore"
  },
  "hela": {
    "extends": "tunnckocore"
  },
  "rollup": {
    "input": "src/index.js",
    "output": [
      "cjs",
      "es"
    ]
  },
  "nyc": {
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "lines": 0,
    "exclude": [
      "test"
    ],
    "extension": [
      ".js",
      ".jsx",
      ".mjs"
    ]
  },
  "version": "0.0.0-semantically-released",
  "repository": "#{repository}",
  "homepage": "https://github.com/#{repository}",
  "author": "Charlike Mike Reagent (www.tunnckocore.com)",
  "cdn": "./dist/index.umd.js",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3,
      "footer": ""
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "gitcommit",
        "try-catch-core",
        "new-release",
        "always-done",
        "jstransformer"
      ],
      "highlight": "charlike-cli"
    },
    "lint": {
      "reflinks": true
    }
  }
}
