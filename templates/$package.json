{
  "name": "#{name}",
  "description": "#{description}",
  "license": "Apache-2.0",
  "licenseStart": "#{date('yyyy')}",
  "scripts": {
    "lint": "eslint src test --fix --quiet",
    "test": "nyc --require esmc/register asia",
    "gitadd": "git status --porcelain && git add -A",
    "commit-dry": "yarn gitadd && gitcommit -sS",
    "precommit": "yarn lint && yarn test && yarn gitadd",
    "commit": "gitcommit -sS",
    "build": "esmc",
    "prepublishOnly": "yarn build"
  },
  "engines": {
    "node": "^8.9.0 || ^10.6.0"
  },
  "dependencies": {},
  "devDependencies": {
    "all-contributors-cli": "^5.4.0",
    "asia": "^0.19.7",
    "esmc": "*",
    "gitcommit": "^1.0.9",
    "new-release": "^4.0.3",
    "nyc": "^12.0.2"
  },
  "files": [
    "dist"
  ],
  "main": "dist/nodejs/__index.js",
  "module": "dist/nodejs/index.js",
  "version": "0.0.0-semantically-released",
  "repository": "#{owner}/#{name}",
  "homepage": "https://github.com/#{owner}/#{name}",
  "author": "Charlike Mike Reagent (https://tunnckocore.com)",
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": "./node_modules/esmc/src/eslint/config.js"
  },
  "renovate": {
    "extends": "tunnckocore"
  },
  "nyc": {
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "lines": 0,
    "cache": true,
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "test"
    ]
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3,
      "footer": ""
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
