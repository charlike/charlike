{
  "name": "charlike",
  "description": "Small & fast project scaffolder with sane defaults. Supports hundreds of template engines through the @JSTransformers API or if you want custom `render` function passed through options",
  "license": "Apache-2.0",
  "licenseStart": "2016",
  "scripts": {
    "cli": "node cli.js",
    "docs": "docks --outfile .verb.md && verb",
    "lint": "eslint src test --cache --fix --quiet --format codeframe",
    "test": "nyc asia",
    "commit-dry": "git status --porcelain && git add -A && gitcommit -sS",
    "precommit": "lint-staged && yarn test",
    "commit": "yarn commit-dry"
  },
  "engines": {
    "node": "^8.11.0 || ^10.12.0"
  },
  "dependencies": {
    "@tunnckocore/package-json": "^0.3.1",
    "camelcase": "^5.0.0",
    "dateformat": "^3.0.3",
    "esm": "^3.0.84",
    "git-config-path": "^1.0.1",
    "git-user-email": "^0.2.2",
    "git-user-name": "^2.0.0",
    "git-username": "^1.0.0",
    "jstransformer": "^1.0.0",
    "jstransformer-j140": "^1.0.0",
    "jstransformer-jstransformer": "^1.1.6",
    "mri": "^1.1.1",
    "parse-git-config": "^2.0.3",
    "stream-copy-dir": "^1.0.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^5.4.0",
    "asia": "^0.19.7",
    "docks": "^0.6.0",
    "eslint": "^5.8.0",
    "eslint-config-esmc": "^1.0.12",
    "gitcommit": "^1.0.9",
    "husky": "^1.1.2",
    "lint-staged": "^8.0.4",
    "new-release": "^4.0.3",
    "nyc": "^13.1.0"
  },
  "files": [
    "src",
    "cli.js",
    "index.js",
    "templates"
  ],
  "bin": "cli.js",
  "main": "index.js",
  "module": "src/index.js",
  "typings": "src/index.d.ts",
  "version": "0.0.0-semantically-released",
  "repository": "tunnckoCoreLabs/charlike",
  "homepage": "https://github.com/tunnckoCoreLabs/charlike",
  "author": "Charlike Mike Reagent (https://tunnckocore.com)",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "eslintConfig": {
    "extends": "esmc"
  },
  "renovate": {
    "extends": "tunnckocore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 4
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "asia",
        "charlike-cli",
        "docks",
        "gitcommit",
        "new-release",
        "jstransformer"
      ],
      "highlight": "charlike-cli"
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "new-release",
      "semantic-release"
    ]
  }
}
