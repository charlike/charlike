{
  "name": "charlike",
  "version": "0.0.0-semantically-released",
  "description": "Small, fast, simple and streaming project scaffolder for myself, but not only. Supports hundreds of template engines through the @JSTransformers API or if you want custom `render` function passed through options",
  "repository": "tunnckoCore/charlike",
  "author": "Charlike Mike Reagent <@tunnckoCore> (https://i.am.charlike.online)",
  "nspId": "e6f9f165-cc4d-41be-ad83-aa54e30caaab",
  "main": "index.js",
  "license": "MIT",
  "licenseStart": 2016,
  "scripts": {
    "deps": "dependency-check . --missing",
    "secure": "nsp check",
    "lint": "eslint {index,test}.js --fix",
    "test": "nyc --reporter lcov node test.js",
    "report": "nyc report",
    "precommit": "npm-run-all --silent -s deps secure lint test",
    "commit": "git status --porcelain && git add --all && gitcommit -s -S",
    "release": "new-release"
  },
  "dependencies": {
    "camelcase": "^4.1.0",
    "dateformat": "^3.0.3",
    "jstransformer": "^1.0.0",
    "jstransformer-j140": "^1.0.0",
    "jstransformer-jstransformer": "^1.1.6",
    "stream-copy-dir": "^1.0.0"
  },
  "devDependencies": {
    "dependency-check": "2.10.0",
    "eslint": "4.18.0",
    "eslint-config-tunnckocore": "1.0.1",
    "gitcommit": "0.2.4",
    "mukla": "0.4.9",
    "new-release": "4.0.1",
    "npm-run-all": "4.1.2",
    "nsp": "2.8.1",
    "nyc": "11.4.1"
  },
  "files": [
    "index.js",
    "templates"
  ],
  "keywords": [],
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3,
      "footer": ""
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "always-done",
        "minibase",
        "try-catch-core",
        "jstransformer",
        "j140",
        "each-promise"
      ],
      "highlight": "charlike-cli"
    },
    "reflinks": [
      "always-done",
      "async-done",
      "base",
      "commitizen",
      "dezalgo",
      "once",
      "standard-version",
      "verb",
      "verb-generate-readme",
      "hela",
      "new-release"
    ],
    "lint": {
      "reflinks": true
    }
  },
  "engines": {
    "node": ">=8.6",
    "npm": ">=5.2",
    "yarn": ">=1.3"
  },
  "nyc": {
    "check-coverage": true,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "lines": 0
  }
}
